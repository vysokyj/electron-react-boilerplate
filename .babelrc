{
  "env": {
    "production": {
      "presets": [
        ["es2015", {"modules": false}],
        "stage-0",
        "react"
      ],
      "plugins": [
        "babel-plugin-transform-runtime"
      ]
    },
    "development": {
      "presets": [
        ["es2015", {"modules": false}],
        "stage-0",
        "react",
        "react-hmre"
      ],
      "plugins": [
        "babel-plugin-transform-runtime",
        "react-hot-loader/babel"
      ]
    }
  }
}
