{
  "name": "electron-react-boilerplate",
  "version": "0.1.0",
  "description": "Boilerplate for ReactJS project with hot code reloading",
  "scripts": {
    "start": "electron .",
    "lint": "eslint src",
    "build": "webpack -p && electron-builder",
    "pack": "webpack -p && electron-builder --dir"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaearon/react-hot-boilerplate.git"
  },
  "main": "main.js",
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "electron",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "author": "Jiri Vysoky <jiri.vysoky@gmail.com> (http://github.com/vysokyj)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vysokyj/electron-react-boilerplate/issues"
  },
  "build": {
    "productName": "ElectronReactBoilerplate",
    "appId": "com.github.vysokyj.ElectronReactBoilerplate",
    "files": [
      "static/**/*",
      "node_modules/**/*",
      "main.js",
      "index.html",
      "package.json"
    ],
    "publish": null,
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "AppImage"
      ],
      "category": "Development"
    }
  },
  "homepage": "https://github.com/vysokyj/electron-react-boilerplate",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.10",
    "electron-builder": "^20.2.0",
    "electron-devtools-installer": "^2.2.3",
    "eslint": "^4.18.1",
    "eslint-plugin-react": "^7.7.0",
    "react-hot-loader": "^4.0.0",
    "style-loader": "^0.20.2",
    "webpack": "^3.5.2",
    "webpack-dev-server": "^2.11.1",
    "yargs": "^11.0.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.21",
    "normalizr": "^3.2.4",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-addons-shallow-compare": "^15.6.2",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0"
  }
}
